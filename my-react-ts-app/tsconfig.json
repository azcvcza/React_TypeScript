{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    
    "jsx": "react",
    "sourceMap": true,
     "noImplicitReturns":true,
     "rootDir": "src",
     "outDir": "dist",
     "strict":true
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules"]
}
